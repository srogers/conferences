.row
  .col-md-12
    = render partial: 'shared/user_nav'

.row
  .col-md-12
    %h4 Your Presentations

    - if @user_presentations.empty?
      You haven't marked any presentations yet
    - else
      %ul
      - @user_presentations.each do |user_presentation|
        - presentation = user_presentation.presentation
        %li.wrapping_list
          = link_to presentation.name, presentation_path(presentation)
          %br
          = presentation.speaker_names
          = presentation.conference_name
